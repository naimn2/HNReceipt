<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <TextView
        android:id="@+id/tv_itemListPanenLahan_lahan"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/normal_margin"
        android:text="@string/lahan"
        android:textColor="@color/black"
        app:layout_constraintEnd_toStartOf="@+id/tv_itemListPanenLahan_jumlahHadir"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_itemListPanenLahan_jumlahHadir"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/normal_margin"
        android:text="@string/jumlah_hadir_default"
        android:textColor="@color/lightGray"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/view_itemListPanenLahan_divider"
        android:layout_width="match_parent"
        android:layout_height="@dimen/divider_thin"
        android:layout_margin="@dimen/normal_margin"
        android:background="@color/veryLightGray"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_itemListPanenLahan_lahan" />

</androidx.constraintlayout.widget.ConstraintLayout>